<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;netstandard2.1</TargetFrameworks>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
    <VersionPrefix>1.0.0</VersionPrefix>
    <Title>An extension library to assist with ASP.NET data protection using MySQL</Title>
    <Description>ASP.NET Core Data Protection MySQL library allows you to use MySQL to store keys generated by ASP.NET's Data Protection API.</Description>
    <Summary>ASP.NET Core Data Protection MySQL library allows you to use MySQL to store keys generated by ASP.NET's Data Protection API.</Summary>
    <Copyright>Copyright 2023 Andrei Nicholson</Copyright>
    <AssemblyTitle>AspNetCore.DataProtection.MySql</AssemblyTitle>
    <AssemblyName>AspNetCore.DataProtection.MySql</AssemblyName>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageId>AspNetCore.DataProtection.MySql</PackageId>
    <Authors>Andrei Nicholson</Authors>
    <Product>AspNetCore.DataProtection.MySql</Product>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>mysql;mysqlconnector;aspnet;dataprotection</PackageTags>
    <PackageProjectUrl>https://github.com/tetsuo13/AspNetCore.DataProtection.MySql</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <LangVersion>10.0</LangVersion>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <PackageReleaseNotes>https://github.com/tetsuo13/AspNetCore.DataProtection.MySql/blob/main/CHANGELOG.md</PackageReleaseNotes>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/tetsuo13/AspNetCore.DataProtection.MySql.git</RepositoryUrl>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>AspNetCore.DataProtection.MySql</RootNamespace>
    <Nullable>enable</Nullable>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\AspNetCore.DataProtection.MySql.snk</AssemblyOriginatorKeyFile>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.DataProtection" Version="7.*" />
    <PackageReference Include="MySqlConnector" Version="2.*" />
  </ItemGroup>

  <ItemGroup>
    <InternalsVisibleTo Include="$(MSBuildProjectName).Tests" Key="" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE" Pack="true" PackagePath="" />
    <None Include="..\..\README.md" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
